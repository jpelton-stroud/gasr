{
  "name": "gasr",
  "version": "0.0.0",
  "description": "gasr - utilities for writing Google Apps Scripts locally via Clasp & Typescript",
  "scripts": {
    "clean": "npx fse emptyDir build",
    "lint": "npx prettier --write src",
    "webpack": "webpack --stats-children",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run lint && npm run webpack",
    "deploy_dev": "npm run build && clasp push",
    "open_app": "clasp open --webapp --deploymentId=AKfycbx5hLisDie5iziK-dHmrIlBp7S_z0UnK4nAIdLAEc8q"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpelton-stroud/gasr.git"
  },
  "author": "Stroud <jpstroud@gmail.com> (https://jpelton-stroud.github.io/)",
  "license": "LGPL-2.1+",
  "bugs": {
    "url": "https://github.com/jpelton-stroud/gasr/issues"
  },
  "homepage": "https://github.com/jpelton-stroud/gasr#readme",
  "dependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@google/clasp": "^2.4.1",
    "@types/google-apps-script": "^1.0.42",
    "@types/jquery": "^3.5.11",
    "copy-webpack-plugin": "^10.2.0",
    "ejs-loader": "^0.5.0",
    "fs-extra": "^10.0.0",
    "gas-webpack-plugin": "^2.1.0",
    "inquirer": "^8.0.0",
    "jquery": "^3.6.0",
    "meow": "^9.0.0",
    "ts-loader": "^9.2.6",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/node": "^15.0.3",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "template-ejs-loader": "^0.5.0"
  }
}
